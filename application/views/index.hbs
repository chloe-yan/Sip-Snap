<h1>‏‏‎ ‎Brewer 💭</h1>
<div id="photo-container">
    {{#each results}}
        {{> card this}}
    {{/each}}
</div>

<script>
    document.addEventListener("DOMContentLoaded", (event) => {
        document.querySelectorAll(".card").forEach((card) => {
            let id = parseInt(card.id.slice(-2));
            let gradient = (id % 3) + 1;
            card.lastElementChild.firstElementChild.classList.add("gradient-" + gradient);
        });
    });
</script>